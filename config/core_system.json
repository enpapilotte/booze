{
    "package_manager": "brew",
    "managers": {
      "apt": {
        "install": "sudo apt install",
        "update": "sudo apt update",
        "remove": "sudo apt remove",
        "search": "apt search",
        "list_installed": "dpkg --list",
        "refresh_db": "sudo apt update"
      },
      "yum": {
        "install": "sudo yum install",
        "update": "sudo yum update",
        "remove": "sudo yum remove",
        "search": "yum search",
        "list_installed": "yum list installed",
        "refresh_db": "sudo yum makecache"
      },
      "dnf": {
        "install": "sudo dnf install",
        "update": "sudo dnf update",
        "remove": "sudo dnf remove",
        "search": "dnf search",
        "list_installed": "dnf list installed",
        "refresh_db": "sudo dnf makecache"
      },
      "pacman": {
        "install": "sudo pacman -S",
        "update": "sudo pacman -Syu",
        "remove": "sudo pacman -R",
        "search": "pacman -Ss",
        "list_installed": "pacman -Q",
        "refresh_db": "sudo pacman -Syy"
      },
      "zypper": {
        "install": "sudo zypper install",
        "update": "sudo zypper update",
        "remove": "sudo zypper remove",
        "search": "zypper search",
        "list_installed": "zypper packages --installed-only",
        "refresh_db": "sudo zypper refresh"
      },
      "apk": {
        "install": "sudo apk add",
        "update": "sudo apk update",
        "remove": "sudo apk del",
        "search": "apk search",
        "list_installed": "apk info",
        "refresh_db": "sudo apk update"
      },
      "brew": {
        "install": "brew install",
        "update": "brew update",
        "remove": "brew uninstall",
        "search": "brew search",
        "list_installed": "brew list",
        "refresh_db": "brew update"
      },
      "snap": {
        "install": "sudo snap install",
        "update": "sudo snap refresh",
        "remove": "sudo snap remove",
        "search": "snap find",
        "list_installed": "snap list",
        "refresh_db": "sudo snap refresh"
      },
      "flatpak": {
        "install": "flatpak install",
        "update": "flatpak update",
        "remove": "flatpak uninstall",
        "search": "flatpak search",
        "list_installed": "flatpak list",
        "refresh_db": "flatpak update"
      },
      "nix": {
        "install": "nix-env -iA",
        "update": "nix-channel --update && nix-env -u",
        "remove": "nix-env -e",
        "search": "nix-env -qa",
        "list_installed": "nix-env -q",
        "refresh_db": "nix-channel --update"
      },
      "port": {
        "install": "sudo port install",
        "update": "sudo port selfupdate",
        "remove": "sudo port uninstall",
        "search": "port search",
        "list_installed": "port installed",
        "refresh_db": "sudo port selfupdate"
      }
    }
  }
  